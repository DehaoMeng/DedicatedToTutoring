<?xml version="1.0" encoding="utf-8"?>
<DirectionalLayout
    xmlns:ohos="http://schemas.huawei.com/res/ohos"
    ohos:height="match_content"
    ohos:width="300vp"
    ohos:orientation="vertical">
    <DirectionalLayout
        ohos:height="match_content"
        ohos:width="match_parent">
        <Text
            ohos:id="$+id:costitl"
            ohos:height="70vp"
            ohos:width="match_parent"
            />
        <!-- 选择框 -->
        <ListContainer
            ohos:id="$+id:coslicont"
            ohos:height="300vpvp"
            ohos:width="match_parent"
            ohos:background_element="$graphic:background_ability"
            />

    </DirectionalLayout>
    
    


</DirectionalLayout>